{
  "manifest_version": 3,
  "name": "My React Extension",
  "version": "1.0",
  "description": "A React-based browser extension",
 "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}

